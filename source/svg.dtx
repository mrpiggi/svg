% It is checked whether a key was given as \val{\meta{key}=\meta{value}} or 
% like \val{\meta{key}=\{\meta{format}=\meta{value}\}}.
%    \begin{macrocode}
\newcommand*\svgx@ifkeyandval[3]{%
  \def\svg@tempa##1=##2=##3\@nil{\ifstr{##3}{=}{#2}{#3}}%
